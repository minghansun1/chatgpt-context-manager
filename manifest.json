{
    "name": "ChatGPT Context Manager",
    "description": "This extension helps you manage your ChatGPT context.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
        {
        "js": ["scripts/content.js"],
        "matches": [
            "https://developer.chrome.com/docs/extensions/*",
            "https://developer.chrome.com/docs/webstore/*",
            "https://chatgpt.com/*"
        ]
        }
    ]
  }